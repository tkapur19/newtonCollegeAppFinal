<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout"
    android:background="@drawable/edu"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.tarunkapur.newtoncollegeapp.Main2Activity">

    <Button
        android:id="@+id/button5"
        android:layout_width="200dp"
        android:layout_height="60dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="35dp"
        android:backgroundTint="@android:color/holo_blue_dark"
        android:text="3:50-4:50"
        android:textSize="@dimen/fab_margin"
        android:textStyle="bold"
        android:onClick="getClass"
        />

    <Button
        android:id="@+id/button6"
        android:layout_width="200dp"
        android:layout_height="60dp"
        android:layout_alignLeft="@+id/button5"
        android:layout_alignStart="@+id/button5"
        android:layout_below="@+id/button5"
        android:layout_marginTop="41dp"
        android:backgroundTint="@android:color/holo_orange_light"
        android:text="4:50-6:50"
        android:textSize="@dimen/fab_margin"
        android:textStyle="bold"
        android:onClick="getClass"
        />

    <Button
        android:layout_width="200dp"
        android:layout_height="60dp"
        android:layout_alignLeft="@+id/button6"
        android:layout_alignStart="@+id/button6"
        android:layout_below="@+id/button6"
        android:layout_marginTop="39dp"
        android:backgroundTint="@android:color/holo_green_dark"
        android:text="5:50-6:50"
        android:textStyle="bold"
        android:textSize="@dimen/fab_margin"
        android:id="@+id/button7"
        android:onClick="getClass"/>

    <Button
        android:id="@+id/button8"
        android:layout_width="200dp"
        android:layout_height="60dp"
        android:layout_alignLeft="@+id/button7"
        android:layout_alignStart="@+id/button7"
        android:layout_below="@+id/button7"
        android:layout_marginTop="41dp"
        android:backgroundTint="@android:color/holo_red_dark"
        android:text="6:50-7:50"
        android:textSize="@dimen/fab_margin"
        android:textStyle="bold"
        android:onClick="getClass"/>

</RelativeLayout>